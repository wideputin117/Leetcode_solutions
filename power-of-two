// function two return n such that it is obtained by power of two

var power= (n)=>{
if(n<=0)
return false;
if(n === 1)
return true;
if(n %2 !== 0)
return false;
console.log(n);
return power(n/2);
}
const ans = power(16);
console.log(ans);

