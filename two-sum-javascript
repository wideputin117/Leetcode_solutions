// function to check for palindrome

function palindromeArr(arr2){
    let len = arr2.length;
 
    for (let i =0; i < len/2;i++){
      if(arr2[i] !== arr2[len-i-1]){
        return false;
      }
    }
    return true;
  }
  console.log(palindromeArr("amama"))

  // finding same element in an array

  function sameElement(arr3){
    let map = new Map();
    let len = arr3.length;
    let same=[];
    for(let i=0;i<len;i++){
        if(map.has(arr3[i])){
             same.push(arr3[i]);
        }else{
            map.set(arr3[i],i)
        }
    }
return same;
  }

  console.log(sameElement([1,2,3,4,5,4,6,1]));

  // javascript program to find uniques values in an array

  function uniqueValues(arr){
    let len = arr.length;
    let uniques={};
    for(let i=0;i<len;i++){
      if(uniques[arr[i]]){
        uniques[arr[i]]++;
      }else{
        uniques[arr[i]]=1;
      }
    }
    let single = Object.keys(uniques).filter((element)=> uniques[element]=== 1);
    return single

  }
  console.log(uniqueValues([2,3,1,3,2,6,4,1]));



